<Window x:Class="PILOTLOGGER.LogBox"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PILOTLOGGER"
        mc:Ignorable="d"
        Title="LogBox" Height="450" Width="800">
    <Grid>
        <RichTextBox x:Name="consolelog" HorizontalAlignment="Left" Height="403" Margin="10,10,0,0" VerticalAlignment="Top" Width="775" TextChanged="RichTextBox_TextChanged" Background="Black" Foreground="White">
            <FlowDocument/>
        </RichTextBox>

    </Grid>
</Window>
